  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "cleanup": "tsc --build ./ --clean && rm -rf node_modules && rm -rf yarn.lock && rm -rf dist && rm -rf cdk.out",
    "cleanup:tsc": "tsc --build ./ --clean",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --coverage",
    "testreport": "jest --ci --coverage",
    "cdk": "cdk",
    "lint": "eslint --fix  --max-warnings 0 -c ../../../.eslintrc.json 'lib/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" ",
    "precommit": "eslint --max-warnings 0 -c ../../../.eslintrc.json 'lib/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}' --ignore-pattern \"*.d.ts\" "
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.123.0",
    "@types/jest": "26.0.10",
    "@types/node": "16.9.1",
    "aws-cdk": "1.123.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "26.4.2",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "ts-jest": "26.2.0",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  },
