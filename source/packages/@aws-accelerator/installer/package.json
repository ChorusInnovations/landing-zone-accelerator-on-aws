    "@aws-cdk/aws-s3-assets": "1.123.0",
    "@aws-cdk/cloud-assembly-schema": "1.123.0",
    "@aws-cdk/core": "1.123.0",
    "@aws-cdk/cx-api": "1.123.0",
    "@aws-cdk/region-info": "1.123.0",
    "@aws-compliant-constructs/compliant-constructs": "^0.0.0"
  }
