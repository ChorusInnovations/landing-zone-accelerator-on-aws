              return v.toString(16);
          });
      }
      
      
      function sanatizeData(resourceProperties) {
          const keysToExclude = ['ServiceToken', 'Resource', 'SolutionId', 'UUID'];
          return Object.keys(resourceProperties).reduce((sanatizedData, key) => {
